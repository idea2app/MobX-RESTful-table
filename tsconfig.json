{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "lib": ["ES2022", "DOM"]
  },
  "include": ["source/*"],
  "typedocOptions": {
    "name": "MobX RESTful table",
    "excludeExternals": true,
    "excludePrivate": true,
    "readme": "./ReadMe.md",
    "out": "docs/"
  }
}
